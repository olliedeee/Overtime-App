<h1> Posts</h1>
<table class="table table-hover">
	<thead>
		<tr>
			<th>
				Daily Hours
			</th>
			<th>
				Date
			</th>
			<th class='hidden-xs'>
				Description
			</th>
			<th>
				Status
			</th>
		</tr>
	</thead>
		<tbody id="posts">    
		  <%= render @posts %>
		</tbody>
	</table>
	<div id="paginator">
		<%= paginate @posts, remote: true %>
	</div>